generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id            Int      @id @default(autoincrement())
  name          String
  birthday      DateTime
  address       String
  age           Int
  gender        String
  contactNumber String
  dateToday     DateTime @default(now())
}
